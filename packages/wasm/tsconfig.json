{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "include": ["test", "src-js"],
  "compilerOptions": {
    "noUncheckedIndexedAccess": false,
    "checkJs": false,
    "noEmit": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "types": ["@types/node"]
  }
}
